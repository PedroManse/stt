#! ./target/debug/stt
(pragma set manual-array)
(pragma set debug)
(include stdlib)

(fn) [a] double { a 2 * }
(fn) [a] add-one { a ++ }
(fn) [ cl-b cl-a ] cl$joinr {
	[ cl-a cl-b x ] {
		cl-b cl-a x @ @
	} cl-a @ cl-b @
}

(fn) [ cl-a cl-b ] cl$joinl {
	[ cl-a cl-b x ] {
		cl-b cl-a x @ @
	} cl-a @ cl-b @
}

(@add-one) (@double) cl$joinr 4 @ prt
(@double) (@add-one) cl$joinl 4 @ prt

(@double) (@add-one) (@double) cl$joinl cl$joinl 4 @ prt

