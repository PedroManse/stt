(fn*) [ ] argv$init {
	sys$argv
	arr$reverse
	arr$unpack
	drop drop drop
	arr$pack
	"_argv" set
}

(fn*) [] argv$pop {
	"_argv" get
	arr$unpack
	1 - arr$pack-n
	"_argv" set
}

(fn*) [] argv$rest {
	"_argv" get
	arr$reverse
	arr$unpack
}

(fn*) [] argv$is-empty {
	"_argv" get
	arr$is-empty
}

(fn*) [] argv$try_pop {
	"_argv" get
	(ifs) { dup arr$is-empty } {
		"empty array" err
	} { true } {
		argv$pop ok
	}
}
